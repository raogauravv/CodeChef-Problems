   "source": [
    "def fac(n):\n",
    "    if n==1 or n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        fact=n*fac(n-1)\n",
    "        return(fact)\n",
    "\n",
    "l=[]\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    a=int(input())\n",
    "    fac(a)\n",
    "    l.append(fac(a))\n",
    "for i in l:\n",
    "    print(i)\n",
    "    "
   ]
