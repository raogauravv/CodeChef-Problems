   "source": [
    "def fact(n):\n",
    "    fac=1\n",
    "    while n>=1:\n",
    "        fac*=n\n",
    "        n-=1\n",
    "    return fac\n",
    "\n",
    "n=int(input())\n",
    "l=[]\n",
    "for i in range(n):\n",
    "    a=int(input())\n",
    "    b=(fact(a))\n",
    "    l.append(b)\n",
    "for i in l:\n",
    "    print(i)\n",
    "    "
